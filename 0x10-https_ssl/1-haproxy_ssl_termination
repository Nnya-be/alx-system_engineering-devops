defaults
	mode http
	timeout client 15s
	timeout connect 10s
	timeout server 15s
	timeout http-request 10s

frontend  chamamme-frontend
        bind *:80
        mode http
        default_backend chamamme-backend
backend chamamme-backend
        balance roundrobin
        server 487279-web-01 100.26.216.148:80 check
        server 487279-web-02 34.201.174.248:80 check

